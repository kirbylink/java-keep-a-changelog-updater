<configuration>
	<appender name="STDOUT"
		class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%msg%n</pattern>
		</encoder>
	</appender>
	
	<logger name="de.dddns.kirbylink.keepachangelogupdater" level="info" />

	<root level="ERROR">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>